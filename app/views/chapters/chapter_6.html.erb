<div class="container">
  <chart-column :columns="<%= @column_chart %>"></chart-column>
</div>

<% @stacked_row_charts[:charts].each do |chart| %>
  <chart-row-stacked title="<%= chart[:chart_title] %>" :rows="<%= chart[:rows].to_json %>"></chart-row-stacked>
<% end %>

<% @data['ctas'].each do |cta| %>
  <%= render partial: '/partials/cta', locals: { title: cta['cta_title'], text: cta['cta_text'], url: cta['cta_url'] } %>
<% end %>